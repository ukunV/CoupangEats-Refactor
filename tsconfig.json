{
  "compilerOptions": {
    "allowJs": true,
    "target": "ES5",
    "outDir": "./dist",
    "moduleResolution": "Node",
    "lib": ["ES2015","ES2016","ES2017","ES2018","ES2019","ES2020","ES2021"],
    "noImplicitAny": true,
    "typeRoots": ["./node_modules/@types", "./types"],
    "esModuleInterop": true,
    "strict": true,
    // "strictFunctionTypes": false
  },
  "include": ["./src/**/*"]
}
